   "source": [
    "program to find factorial of given number\n",
    "\n",
    "Input  : n = 5\n",
    "\n",
    "Output : 120"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "120\n"
     ]
    }
   ],
   "source": [
    "def fact(n):\n",
    "    if n==0:\n",
    "        return 1\n",
    "    return n* fact(n-1)\n",
    "n = int(input(\"Enter a number\"))\n",
    "print(fact(n))\n"
   ]
  }
