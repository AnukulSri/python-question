   "source": [
    "Find all factors of a Natural Number in sorted order\n",
    "\n",
    "Input : n = 10       \n",
    "\n",
    "Output: 1 2 5 10\n",
    "\n",
    "\n",
    "Input:  n = 100\n",
    "\n",
    "Output: 1 2 4 5 10 20 25 50 100"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "1 2 4 8 16 32 "
     ]
    }
   ],
   "source": [
    "n = int(input(\"Enter a natural number\"))\n",
    "for i in range(1,n+1):\n",
    "    if n%i ==0:\n",
    "        print(i,end=' ')"
   ]
  }
