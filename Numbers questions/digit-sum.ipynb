    "Program for Sum of the digits of a given number\n",
    "\n",
    "Input: n = 687\n",
    "\n",
    "Output: 21\n",
    "\n",
    "Input: n = 12\n",
    "\n",
    "Output: 3"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "4\n"
     ]
    }
   ],
   "source": [
    "n = int(input(\"Enter a number\"))\n",
    "s=0\n",
    "a=n\n",
    "while a>0:\n",
    "    mod = a%10\n",
    "    s += mod\n",
    "    a =a//10\n",
    "print(s)\n"
   ]
  }
